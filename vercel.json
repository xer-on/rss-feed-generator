{
  "version": 2,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/src/app.ts"
    }
  ],
  "headers": [
    {
      "source": "/rss/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        },
        {
          "key": "Content-Type",
          "value": "application/atom+xml; charset=utf-8"
        }
      ]
    }
  ],
  "cleanUrls": true
}
